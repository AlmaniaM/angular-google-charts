{
  "extends": [
    "tslint-angular",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "node_modules/codelyzer"
  ],
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "kebab-case"
    ],
    "ordered-imports": true,
    "member-ordering": true,
    "member-access": true,
    "array-type": [
      true,
      "array"
    ]
  }
}