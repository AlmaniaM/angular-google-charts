{
  "name": "angular-google-charts",
  "description": "A wrapper for the Google Charts library written in Angular",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --prod angular-google-charts",
    "test": "ng test angular-google-charts",
    "lint": "ng lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": "tslint --fix",
    "*.{json,html,ts,js,scss,css,yml,md}": "prettier --write"
  },
  "dependencies": {
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "core-js": "^3.6.4",
    "rxjs": "^6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^9.0.1",
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "^9.1.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/google.visualization": "^0.0.52",
    "@types/jest": "^25.2.1",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "jest-preset-angular": "^8.1.3",
    "lint-staged": "^10.1.1",
    "ng-packagr": "^9.0.0",
    "prettier": "^2.0.2",
    "ts-jest": "^25.3.1",
    "ts-node": "~8.8.1",
    "tslint": "^6.1.1",
    "tslint-angular": "^3.0.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.8.3"
  }
}
